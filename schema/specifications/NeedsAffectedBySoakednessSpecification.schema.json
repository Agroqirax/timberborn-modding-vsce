{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Needs affected by soakedness specification",
  "description": "DEPRECATED, use 'Blueprints/Needs/Need.TYPE.NEED.json' instead\nAdd or modify a need affected by soakedness",
  "type": "object",
  "deprecated": true,
  "properties": {
    "NeedId": {
      "type": "string",
      "description": "Need Id to be modified",
      "minLength": 1,
      "examples": [
        "WetFur"
      ],
      "pattern": "^[a-zA-Z0-9_-]+$"
    },
    "PointsPerHour": {
      "type": "number",
      "description": "Points added /hr",
      "default": 0.5
    }
  },
  "required": [
    "NeedId",
    "PointsPerHour"
  ]
}